{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch ServiceTrack.Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ServiceTrack.Api/bin/Debug/net9.0/ServiceTrack.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ServiceTrack.Api",
            "stopAtEntry": false,
            "console": "internalConsole",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "Attach to Process",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
} 